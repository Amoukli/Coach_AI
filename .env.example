# Coach - Environment Variables Example
# Copy this file to .env and fill in your values

# ===== Backend Environment Variables =====

# Database
DATABASE_URL=postgresql://coach_user:coach_password@localhost:5432/coach_db
DATABASE_URL_ASYNC=postgresql+asyncpg://coach_user:coach_password@localhost:5432/coach_db

# Azure Speech Services
AZURE_SPEECH_KEY=your_azure_speech_key_here
AZURE_SPEECH_REGION=uksouth

# Azure OpenAI
AZURE_OPENAI_KEY=your_azure_openai_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4

# Azure AD B2C
AZURE_AD_TENANT_ID=your_tenant_id
AZURE_AD_CLIENT_ID=your_client_id
AZURE_AD_CLIENT_SECRET=your_client_secret

# External API Integration
CLARE_API_URL=https://www.clareai.app/api
CLARE_API_KEY=your_clare_api_key
CLARK_API_URL=https://www.clarkai.app/api
CLARK_API_KEY=your_clark_api_key

# ElevenLabs (fallback voice generation)
ELEVENLABS_API_KEY=your_elevenlabs_key

# Security
JWT_SECRET_KEY=your-super-secret-jwt-key-change-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Application Settings
ENVIRONMENT=development
DEBUG=True
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# ===== Frontend Environment Variables =====

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000

# Azure AD B2C
VITE_AZURE_AD_CLIENT_ID=your_client_id
VITE_AZURE_AD_TENANT=your_tenant
VITE_AZURE_AD_AUTHORITY=https://your-tenant.b2clogin.com/your-tenant.onmicrosoft.com
VITE_AZURE_AD_REDIRECT_URI=http://localhost:3000

# Environment
VITE_ENV=development
