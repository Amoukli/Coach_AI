# Database
DATABASE_URL=postgresql://user:password@localhost:5432/coach_db
DATABASE_URL_ASYNC=postgresql+asyncpg://user:password@localhost:5432/coach_db

# Azure Speech Services
AZURE_SPEECH_KEY=your_key_here
AZURE_SPEECH_REGION=uksouth

# Azure OpenAI
AZURE_OPENAI_KEY=your_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4

# Azure AD B2C
AZURE_AD_TENANT_ID=your_tenant_id
AZURE_AD_CLIENT_ID=your_client_id
AZURE_AD_CLIENT_SECRET=your_client_secret

# External APIs
CLARE_API_URL=https://www.clareai.app/api
CLARE_API_KEY=your_clare_api_key
CLARK_API_URL=https://www.clarkai.app/api
CLARK_API_KEY=your_clark_api_key

# ElevenLabs (fallback voice)
ELEVENLABS_API_KEY=your_elevenlabs_key

# Security
JWT_SECRET_KEY=your-super-secret-jwt-key-change-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Application
ENVIRONMENT=development
DEBUG=True
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]
